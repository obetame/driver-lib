  "dependencies": {
    "babel-polyfill": "6.23.0",
    "hogan.js": "3.0.2",
    "jquery": "3.2.1",
    "js-yaml": "3.9.1",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "riot": "3.6.1",
    "rollup": "0.45.2",
    "rollup-plugin-commonjs": "8.0.2",
